{
    "entry": "examples/test-project/index.php",
    "output": "build/packed.php",
    "database": "build/packer.db",
    "include_paths": [
        "examples/test-project/src",
        "examples/test-project/lib"
    ],
    "exclude_patterns": [
        "**/tests/**",
        "**/Tests/**",
        "**/*Test.php",
        "**/vendor/**"
    ],
    "autoload": {
        "psr-4": {
            "TestApp\\": "examples/test-project/src/"
        }
    },
    "optimization": {
        "remove_comments": true,
        "remove_whitespace": false,
        "inline_includes": true
    },
    "runtime": {
        "error_reporting": "E_ALL",
        "memory_limit": "256M",
        "timezone": "Asia/Shanghai"
    },
    "version": "1.0.0"
}