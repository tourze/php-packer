{
  "name": "tourze/php-packer",
  "description": "A PHP library for analyzing, packing and merging PHP files using AST storage",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-PDO": "*",
    "ext-tokenizer": "*",
    "monolog/monolog": "^3.1",
    "nikic/php-parser": "^5.5.0",
    "psr/log": "^3|^2|^1",
    "symfony/stopwatch": "^7.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "rector/rector": "^2.1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "PhpPacker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPacker\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/php-packer"
  ]
}